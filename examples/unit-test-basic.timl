datatype printable =
         S {len : Nat | len <= 361} of string {len} --> printable
       | I of int --> printable
       | Ln

fun print_printable a return using 1811.0 =
  case a of
      S s => print s
    | I i => print_int i
    | Ln => println ""
                   
fun printls {len : Nat} (ls : list _ {len}) = List.app print_printable ls
                                                
